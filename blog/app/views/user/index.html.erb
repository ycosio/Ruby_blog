<h1>Users</h1>

<table>
  <tr>
    <th>id</th>
    <th>Name</th>
    <th>Last name</th>
    <th>Email</th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.email %></td>
      <td><%= link_to 'Show', user_path(user)%>
     <td><%= link_to 'Edit', edit_user_path(user) %>
    </tr>
  <% end %>
</table>

<br><%= link_to 'New', new_user_path  %>





